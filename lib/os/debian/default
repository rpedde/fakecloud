# -*- mode: shell-script -*-

EXTRA_PACKAGES=${DEBIAN_EXTRA_PACKAGES:-emacs23-nox sudo}

function valid_images() {
    _RETVAL=""
    for release in squeeze etch; do
	_RETVAL+="debian-${release} "
    done
}

function get_mirror() {
    # $1 - dist
    # $2 - release

    _RETVAL=${DEBIAN_MIRROR:-http://ftp.us.debian.org/debian}
}

